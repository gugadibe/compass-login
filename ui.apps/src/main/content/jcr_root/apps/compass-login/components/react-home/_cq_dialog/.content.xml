<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="React - Home Page"
          sling:resourceType="cq/gui/components/authoring/dialog">
   <content jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
      <layout jcr:primaryType="nt:unstructured"
              margin="{Boolean}false"
              sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
      <items jcr:primaryType="nt:unstructured">
         <column jcr:primaryType="nt:unstructured"
                 sling:resourceType="granite/ui/components/coral/foundation/container">
            <items jcr:primaryType="nt:unstructured">
                <title1 cq-msm-lockable="./title1"
                        fieldLabel="Digite o primeiro título"
                        jcr:primaryType="nt:unstructured"
                        name="./title1"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                <text1 cq-msm-lockable="./text1"
                        fieldLabel="Digite o primeiro texto"
                        jcr:primaryType="nt:unstructured"
                        name="./text1"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                <title2 cq-msm-lockable="./title2"
                        fieldLabel="Digite o segundo título"
                        jcr:primaryType="nt:unstructured"
                        name="./title2"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                <text2 cq-msm-lockable="./text2"
                        fieldLabel="Digite o segundo texto"
                        jcr:primaryType="nt:unstructured"
                        name="./text2"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                <title3 cq-msm-lockable="./title3"
                        fieldLabel="Digite o terceiro título"
                        jcr:primaryType="nt:unstructured"
                        name="./title3"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                <text3 cq-msm-lockable="./text3"
                        fieldLabel="Digite o terceiro texto"
                        jcr:primaryType="nt:unstructured"
                        name="./text3"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                <title4 cq-msm-lockable="./title4"
                        fieldLabel="Digite o quarto título"
                        jcr:primaryType="nt:unstructured"
                        name="./title4"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                <text4 cq-msm-lockable="./text4"
                        fieldLabel="Digite o quarto texto"
                        jcr:primaryType="nt:unstructured"
                        name="./text4"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
            </items>
        </column>
      </items>
   </content>
</jcr:root>
